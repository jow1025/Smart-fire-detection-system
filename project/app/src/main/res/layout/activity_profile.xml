<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/blue_sky"
    tools:context=".ProfileActivity">



        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/orange"

            android:src="@drawable/fire_front"/>
    <TextView
        android:id="@+id/time_text"
        android:layout_width="389dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/rect"
        android:gravity="center"
        android:text="-"
        android:textColor="@android:color/black"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/data_text"
        android:layout_width="389dp"
        android:layout_height="117dp"
        android:layout_below="@+id/status_text"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/rect"
        android:gravity="center"
        android:text="-"
        android:textColor="@android:color/black"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/status_text"
        android:layout_width="389dp"
        android:layout_height="117dp"
        android:layout_below="@+id/time_text"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:background="@drawable/rect"
        android:gravity="center"
        android:text="-"
        android:textColor="@android:color/black"
        android:textSize="30sp" />
     <ImageView
        android:id="@+id/dustLogo"
        android:src="@drawable/ic_action_time"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/time_text"
        android:layout_alignLeft="@+id/time_text"
        android:layout_alignTop="@+id/time_text"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="29dp" />

    <ImageView
        android:id="@+id/daLogo"
        android:src="@drawable/ic_action_gas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/status_text"
        android:layout_alignLeft="@+id/status_text"
        android:layout_alignTop="@+id/status_text"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="29dp" />

    <ImageView
        android:id="@+id/gasLogo"
        android:src="@drawable/ic_action_gas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/data_text"
        android:layout_alignLeft="@+id/data_text"
        android:layout_alignTop="@+id/data_text"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="29dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/storage"
        android:src="@drawable/ic_action_storage"
        android:layout_below="@+id/data_text"
        android:background="@drawable/oval"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/analysis"
        android:src="@drawable/ic_action_anal"
        android:layout_below="@+id/data_text"
        android:background="@drawable/oval"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cctv"
        android:src="@drawable/ic_action_cctv"
        android:layout_below="@+id/data_text"
        android:background="@drawable/oval"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/logout"
        android:src="@drawable/ic_action_logout"
        android:layout_below="@+id/data_text"
        android:background="@drawable/oval"/>
</RelativeLayout>
